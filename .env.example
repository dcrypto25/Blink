# Demo Mode (SECURITY: NEVER enable in production!)
# Bypasses passkey authentication for easier development testing
# Only works in development mode
VITE_ENABLE_DEMO_MODE=true

# Solana RPC URL (use devnet for development)
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com

# $BLINK Token Address (to be deployed)
VITE_BLINK_TOKEN_ADDRESS=

# Optional: Custom RPC for better performance
# VITE_SOLANA_RPC_URL=https://rpc.helius.xyz/?api-key=YOUR_KEY

# Multiple RPC endpoints for failover (comma-separated)
# VITE_RPC_ENDPOINTS=https://api.devnet.solana.com,https://devnet.helius-rpc.com
